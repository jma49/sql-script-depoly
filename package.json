{
  "name": "sql_script_depoly",
  "version": "0.5.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:clean": "npm run clean && npm run build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "dev:clean": "npm run clean:cache && npm run dev",
    "sql:check": "ts-node scripts/run-sql.ts check-square-order-duplicates",
    "sql:run": "ts-node scripts/run-sql.ts",
    "sql:run-all": "ts-node -r dotenv/config scripts/run-all-scripts.ts all",
    "sql:run-scheduled": "ts-node -r dotenv/config scripts/run-all-scripts.ts scheduled",
    "sql:run-enabled": "ts-node -r dotenv/config scripts/run-all-scripts.ts enabled",
    "scheduler": "ts-node -r dotenv/config scripts/scheduler/task-scheduler.ts",
    "scheduler:start": "chmod +x scripts/scheduler/start-scheduler.sh && ./scripts/scheduler/start-scheduler.sh",
    "scheduler:manage": "chmod +x scripts/manage-scheduler.sh && ./scripts/manage-scheduler.sh",
    "test:scheduler": "ts-node -r dotenv/config scripts/test-schedule-utils.ts",
    "test:db": "NODE_OPTIONS='--import=ts-node/register --loader=ts-node/esm' node --no-warnings test/database/connection.test.ts",
    "test:notify": "ts-node test/notifications/slack.test.ts",
    "test:mongodb": "ts-node test/database/mongodb.test.ts",
    "optimize:db": "ts-node -r dotenv/config scripts/maintenance/optimize-database-indexes.ts",
    "optimize:db:report": "ts-node -r dotenv/config scripts/maintenance/optimize-database-indexes.ts --report-only",
    "optimize:db:dry-run": "ts-node -r dotenv/config scripts/maintenance/optimize-database-indexes.ts --dry-run",
    "cache:health": "ts-node -r dotenv/config scripts/manage-cache.ts health",
    "cache:stats": "ts-node -r dotenv/config scripts/manage-cache.ts stats",
    "cache:clear": "ts-node -r dotenv/config scripts/manage-cache.ts clear",
    "cache:monitor": "ts-node -r dotenv/config scripts/manage-cache.ts monitor",
    "cache:strategies": "ts-node -r dotenv/config scripts/manage-cache.ts strategies",
    "frontend:build": "npm run build",
    "frontend:analyze": "ANALYZE=true npm run build",
    "security:deploy": "ts-node -r dotenv/config scripts/security-and-performance/deploy-enhanced-security-lite.ts",
    "security:deploy-sql-only": "ts-node -r dotenv/config scripts/security-and-performance/deploy-enhanced-security-lite.ts --security-only",
    "security:deploy-pool-only": "ts-node -r dotenv/config scripts/security-and-performance/deploy-enhanced-security-lite.ts --pool-only",
    "security:deploy-no-tests": "ts-node -r dotenv/config scripts/security-and-performance/deploy-enhanced-security-lite.ts --no-tests",
    "test:check-history-api": "curl -s 'http://localhost:3000/api/check-history?optimized=true&page=1&limit=10' | jq .",
    "test:check-history-cache": "curl -s 'http://localhost:3000/api/check-history?optimized=true&page=1&limit=5' && echo '\n--- Second call (should be faster) ---' && curl -s 'http://localhost:3000/api/check-history?optimized=true&page=1&limit=5'"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.20.2",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "@uiw/codemirror-theme-dracula": "^4.23.12",
    "@uiw/codemirror-theme-eclipse": "^4.23.12",
    "@uiw/codemirror-theme-github": "^4.23.12",
    "@uiw/codemirror-theme-material": "^4.23.12",
    "@uiw/codemirror-theme-nord": "^4.23.12",
    "@uiw/codemirror-theme-okaidia": "^4.23.12",
    "@uiw/codemirror-theme-solarized": "^4.23.12",
    "@uiw/codemirror-theme-tokyo-night": "^4.23.12",
    "@uiw/react-codemirror": "^4.23.12",
    "@upstash/redis": "^1.35.0",
    "@vercel/blob": "^1.0.1",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.3.0",
    "express": "^5.1.0",
    "lucide-react": "^0.487.0",
    "mongodb": "^6.15.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.0",
    "pg": "^8.14.1",
    "pg-formatter": "^2.0.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "sql-formatter": "^15.6.2",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22.15.29",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  }
}
